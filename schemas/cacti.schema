  MUST ( cn $ cactiRealmAccess )
  MAY  ( cactiShowTree $ cactiShowList $
         cactiShowPreview $ cactiGraphSettings $
         cactiLoginOpts $ cactiPolicyGraphs $
         cactiPolicyTrees $ cactiPolicyHosts $
         cactiGraphTemplates $ cactiItemID )
