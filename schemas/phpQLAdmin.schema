objectclass ( 1.3.6.1.4.1.8767.3.1.1
	NAME 'phpQLAdmin'
	DESC 'phpQLAdmin configuration values'
	MAY (	showUsers $ autoReload $ autoSend $
		useControls $ autoAddLocals $ useEzmlm $
		allowServerChange $ allowAbsoluteMailPath $
		deleteVerification $ whoAreWe $ ldapHost $
		subTreeUsers $ subTreeGroups $ branchObjectClass $
		userObjectClass $ userReference $ ezmlmBinaryPath $
		passwordScheme $ hostMaster $ testMailSubject $
		testMailText) )

